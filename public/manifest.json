{
  "manifest_version": 3,
  "name": "NBA Fantasy Extension",
  "version": "1.0.1",
  "description": "Enhance the NBA Fantasy website",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.nbafantasy.nba.com/*"],
      "js": ["content.js"]
    }
  ]
}
